      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        " \n",
        "def sigmoid(x):\n",
        "    return 1 / (1 +np.exp(-x))\n",
        "  \n",
        "def derivate_sigmoid(x):\n",
        "  return sigmoid(x)*(1-sigmoid(x))"
      ],
      "execution_count": 0,
